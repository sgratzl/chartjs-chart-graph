{
  "name": "chartjs-chart-graph",
  "description": "Chart.js module for charting error bars",
  "version": "3.0.0-alpha.19",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samu@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/chartjs-chart-graph.git"
  },
  "main": "build/Chart.Graphs.js",
  "unpkg": "build/Chart.Graphs.min.js",
  "module": "build/Chart.Graphs.esm.js",
  "sideEffects": false,
  "files": [
    "build",
    "src/**/*.js"
  ],
  "peerDependencies": {
    "@sgratzl/chartjs-esm-facade": "^3.0.0-alpha.2",
    "chart.js": "^3.0.0-alpha.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "d3-force": "^2.0.1",
    "d3-hierarchy": "^1.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@sgratzl/chartjs-esm-facade": "^3.0.0-alpha.2",
    "@yarnpkg/pnpify": "^2.1.0",
    "babel-jest": "^26.1.0",
    "canvas": "^2.6.1",
    "chart.js": "^3.0.0-alpha.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "jest-image-snapshot": "^4.0.2",
    "prettier": "^2.0.5",
    "release-it": "^13.6.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.0",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "clean": "rimraf build *.tgz",
    "watch": "rollup -c -w",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "posttest": "npm run lint",
    "build:dev": "rollup -c",
    "build:prod": "npm run build:dev && uglifyjs build/Chart.Graphs.js -c -m -o build/Chart.Graphs.min.js",
    "prebuild": "npm run clean && npm test",
    "build": "npm run build:prod",
    "format": "prettier --write samples src package.json *.js README.md",
    "preversion": "npm run test",
    "prepare": "npm run build:dev",
    "prepublishOnly": "npm run build:prod",
    "release": "release-it --disable-metrics --npm.skipChecks",
    "release:pre": "release-it --disable-metrics --npm.skipChecks --preRelease=alpha --npm.tag=next",
    "samples": "cd samples_treeshake && rollup -c"
  }
}
