{
  "name": "chartjs-chart-graph",
  "description": "Chart.js module for charting graphs",
  "version": "4.2.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/sgratzl/chartjs-chart-graph",
  "bugs": {
    "url": "https://github.com/sgratzl/chartjs-chart-graph/issues"
  },
  "keywords": [
    "chart.js",
    "graph",
    "force-directed",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/chartjs-chart-graph.git"
  },
  "global": "ChartGraphs",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "require": "build/index.cjs",
  "umd": "build/index.umd.js",
  "unpkg": "build/index.umd.min.js",
  "jsdelivr": "build/index.umd.min.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "scripts": "./build/index.umd.min.js",
      "types": "./build/index.d.ts"
    },
    "./.pnp.cjs": "./.pnp.cjs"
  },
  "sideEffects": false,
  "files": [
    "build",
    "src/**/*.ts"
  ],
  "peerDependencies": {
    "chart.js": "^4.1.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "dependencies": {
    "@types/d3-force": "^3.0.4",
    "@types/d3-hierarchy": "^3.1.2",
    "d3-dispatch": "^3.0.1",
    "d3-force": "^3.0.0",
    "d3-hierarchy": "^3.1.2",
    "d3-quadtree": "^3.0.1",
    "d3-timer": "^3.0.1"
  },
  "devDependencies": {
    "@chiogen/rollup-plugin-terser": "^7.1.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.2",
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/node": "^20.3.1",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@yarnpkg/sdks": "^2.7.1",
    "canvas": "^2.11.2",
    "canvas-5-polyfill": "^0.1.5",
    "chart.js": "^4.3.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-image-snapshot": "^6.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "~3.25.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.3.0",
    "seedrandom": "^3.0.5",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^4.0.0-next.16",
    "typedoc-vitepress-theme": "^1.0.0-next.2",
    "typescript": "^5.1.6",
    "vitepress": "^1.0.0-beta.5",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0"
  },
  "scripts": {
    "clean": "rimraf --glob build docs node_modules \"*.tgz\" \"*.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.c.json",
    "start": "yarn run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch": "yarn run test --watch",
    "test:coverage": "yarn run test --coverage",
    "lint": "yarn run eslint && yarn run prettier",
    "fix": "yarn run eslint:fix && yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "eslint": "eslint src --ext .ts,.tsx --cache",
    "eslint:fix": "yarn run eslint --fix",
    "prepare": "yarn run build",
    "docs:api": "typedoc --options typedoc.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "yarn run docs:api && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "packageManager": "yarn@3.6.0"
}
